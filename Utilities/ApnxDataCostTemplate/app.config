<?xml version="1.0"?>
<configuration>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <appSettings>
    <add key="AppNexus.Endpoint" value="$(AppNexusEndpoint)"/>
    <add key="AppNexus.Username" value="$(AppNexusUsername)"/>
    <add key="AppNexus.Password" value="$(AppNexusPassword)"/>
    <add key="AppNexus.Timeout" value="00:00:05"/>
    <add key="AppNexus.Retries" value="20"/>
    <add key="AppNexus.RetryWait" value="00:00:01"/>
    <add key="AppNexus.IsApp" value="false"/>
    <add key="PersistentDictionary.DefaultType" value="Sql"/>
  </appSettings>
</configuration>
